<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>
    
    <%= f.label :email, 'Email' %>
    <%= f.text_field :email %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    
    <%= f.submit 'ログインする' %>
    
<% end %>
